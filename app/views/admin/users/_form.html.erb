<%= form_for [:admin, @user] do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </p>

  <p>
    <%= f.label :password %>
    <%= f.password_field :password %>
  </p>

  <p>
    <%= f.check_box :is_admin %>
    <%= f.label :is_admin, "Is admin?" %>
  </p>

  <%= f.submit %>
<% end %>
 
